<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Revia Race - biotech</li>
		</mods>
		<match Class="PatchOperationAdd">
			<xpath>/Defs/HediffDef[defName="ReviaRaceSoulreapTier9"]/renderNodeProperties</xpath>
			<value>
				<li Class="Shashlichnik.PawnRenderNodeProperties_Animated">
					<!--Tail 9-->
					<debugLabel>Tail9</debugLabel>
					<texPath>Things/Animated/Tail/Tail1/1</texPath>
					<colorType>Hair</colorType>
					<overrideMeshSize>(1, 1)</overrideMeshSize>
					<parentTagDef>Body</parentTagDef>
					<rotDrawMode>Fresh, Rotting</rotDrawMode>
					<shaderTypeDef>CutoutComplex</shaderTypeDef>
					<subworkerClasses>
						<li>Shashlichnik.PawnRenderSubWorker_RotateByRot</li>
					</subworkerClasses>
					<drawData>
						<scaleOffsetByBodySize>true</scaleOffsetByBodySize>
						<scale>1.77</scale>
						<defaultData>
							<rotationOffset>46</rotationOffset>
							<layer>-13</layer>
						</defaultData>
						<dataNorth>
							<offset>(-0.09, 0.03, -0.57)</offset>
							
						</dataNorth>
						<dataSouth>
							<offset>(-0.05, 0, -0.12)</offset>
						</dataSouth>
						<dataEast>
							<offset>(-0.20, 0, -0.57)</offset>
						</dataEast>
						<dataWest>
							<offset>(0.20, 0, -0.45)</offset>
						</dataWest>
					</drawData>
					<keyframeLines>
						<li>
							<tickOffset>24</tickOffset>
							<keyframesTexPath>Things/Animated/Tail/Tail1/</keyframesTexPath>
							<keyframesCount>40</keyframesCount>
						</li>
						<li>
							<keyframesTexPath>Things/Animated/Tail/Right4/</keyframesTexPath>
							<keyframesCount>70</keyframesCount>
						</li>
					</keyframeLines>
				</li>
			</value>
		</match>
		<success>Always</success>
	</Operation>
</Patch>